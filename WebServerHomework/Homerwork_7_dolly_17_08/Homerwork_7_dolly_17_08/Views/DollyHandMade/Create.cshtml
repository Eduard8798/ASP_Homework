@model Homerwork_7_dolly_17_08.Models.DollyClothesModel

@{
ViewBag.Title = "title";
Layout = "_Layout";
}

<h2>Add Clothes</h2>

<form asp-action="Create" enctype="multipart/form-data">
    <div class="form-group"><input asp-for="Name"/></div>
    <div class="form-group"><input asp-for="Url" /></div>
    <div class="form-group"><input asp-for="Style"/></div>

    <div class="form-group">
        <input type="file" name="file" accept="image/png, image/jpeg"/>
    </div>

    <div class="form-group">
        <input type="submit"/>
    </div>
</form>


